# Test Environment Configuration
NODE_ENV=test
PORT=3001

# Test Database
POSTGRES_USER=test_user
POSTGRES_PASSWORD=test_password
POSTGRES_DB=test_db
DATABASE_URL=postgresql://test_user:test_password@localhost:5432/test_db

# Test Redis
REDIS_PASSWORD=test_redis_password
REDIS_URL=redis://:test_redis_password@localhost:6379

# Test n8n Configuration
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=test_admin
N8N_BASIC_AUTH_PASSWORD=test_admin_password
N8N_ENCRYPTION_KEY=test_encryption_key_minimum_32_characters_long_for_security
N8N_USER_MANAGEMENT_JWT_SECRET=test_jwt_secret_minimum_32_characters_long_for_security
N8N_LOG_LEVEL=error

# Test API Keys (mock values)
ELEVENLABS_API_KEY=test_elevenlabs_key
LEONARDO_API_KEY=test_leonardo_key
SOUNDSTRIPE_API_KEY=test_soundstripe_key
RUNWAY_API_KEY=test_runway_key
SUNO_API_KEY=test_suno_key
KLING_API_KEY=test_kling_key

# Test Third-Party APIs
SUNO_API_URL=http://localhost:3002/mock/suno
KLING_API_URL=http://localhost:3002/mock/kling

# Test File Storage
ASSETS_STORAGE_PATH=./test-assets
FINAL_DRAFT_IMPORT_PATH=./test-assets/scripts
MEDIA_EXPORT_PATH=./test-assets/media
PREMIERE_PROJECTS_PATH=./test-assets/premiere-projects

# Test MCP Server Configuration
MCP_FINAL_DRAFT_PORT=8011
MCP_PREMIERE_PRO_PORT=8012
MCP_ORCHESTRATION_PORT=8013

# Test Logging
LOG_LEVEL=error
LOG_FILE_PATH=./test-logs/app.log
ENABLE_METRICS=false

# Test Cost Monitoring
DAILY_BUDGET_LIMIT=10.00
WEEKLY_BUDGET_LIMIT=50.00
MONTHLY_BUDGET_LIMIT=200.00
COST_ALERT_EMAIL=test@example.com

# Test Security
JWT_SECRET=test_jwt_secret_minimum_32_characters_long_for_testing
CORS_ORIGIN=http://localhost:3001
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=1000

# Test Development Flags
ENABLE_DEBUG_LOGGING=false
MOCK_API_RESPONSES=true
SKIP_API_RATE_LIMITS=true

# Test GitHub Configuration
GITHUB_TOKEN=test_github_token
GITHUB_REPO=test/test-repo